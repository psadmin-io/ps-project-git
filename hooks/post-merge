#!/bin/sh

# vars
datetime="todo"
PS_PROJ_USER="PS"
PS_PROJ_PASS="PS"
PS_PROJ_DB="PSFTDB"

# find project dirs
projs=($(find . -maxdepth 1 -type d | cut -c 3-))
# import projects into DB
for proj in ${projs[@]}
do
	logname="$proj-MERGE-$datetime.log"
	cmd.exe /c "PSIDE.EXE -HIDE -PJFF $proj -FP . -CT ORACLE -CD $PS_PROJ_DB -CO $PS_PROJ_USER -CP $PS_PROJ_PASS -QUIT -LF ../logs/$logname"
done